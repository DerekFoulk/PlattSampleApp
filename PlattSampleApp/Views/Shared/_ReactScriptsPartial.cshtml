<environment include="Development">
    <script src="~/lib/react/umd/react.development.js"></script>
    <script src="~/lib/react-dom/umd/react-dom.development.js"></script>
</environment>
<environment exclude="Development">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/react/16.6.1/umd/react.production.min.js"
            asp-fallback-src="~/lib/react/umd/react.production.min.js"
            asp-fallback-test="window.React"
            crossorigin="anonymous"
            integrity="sha256-RVzHuqdnrlXGn844dxjufcr9/IbQupyEtfNoTYV1RsE=">
    </script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/react-dom/16.6.1/umd/react-dom.production.min.js"
            asp-fallback-src="~/lib/react-dom/umd/react-dom.production.min.js"
            asp-fallback-test="window.React && window.ReactDOM"
            crossorigin="anonymous"
            integrity="sha256-XD41AuG+xTsNpa/Ch8WzDM6iCtl7MMJSx6N9v3zPRX8=">
    </script>
</environment>
